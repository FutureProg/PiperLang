{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug C/C++",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/src/grammar/grammar",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "gdb",	
			"logging": {
				"engineLogging": true
			},
			"setupCommands": [
				{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": false	
				}
			]
    	}
	]
}